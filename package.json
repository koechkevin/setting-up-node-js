{
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "email-templates": "^5.0.4",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "express": "^4.16.4",
    "express-generator": "^4.16.0",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.4.0",
    "jwt-decode": "^2.2.0",
    "nodemailer": "^5.1.1",
    "nodemon": "^1.18.9",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0"
  },
  "name": "setting-up-node-js",
  "version": "1.0.0",
  "description": "setting up an empty node js application",
  "main": "src/index.js",
  "repository": "https://github.com/koechkevin/setting-up-node-js.git",
  "author": "koechkevin <koechkevin92@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build-server": "babel src --out-dir dist -s",
    "clean": "rm -rf dist && mkdir dist",
    "build": "yarn run clean && yarn run build-server",
    "serve": "node dist/index.js",
    "start": "NODE_ENV=production yarn run build && yarn run serve",
    "dev": "NODE_ENV=development  DEBUG=log  source .env && nodemon --exec babel-node src/index.js",
    "migrate": "source .env && node_modules/.bin/sequelize db:migrate",
    "seed": "node_modules/.bin/sequelize db:seed:all"
  }
}
