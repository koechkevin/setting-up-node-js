{
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "express": "^4.16.4",
    "express-generator": "^4.16.0",
    "nodemon": "^1.18.9",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0"
  },
  "name": "setting-up-node-js",
  "version": "1.0.0",
  "description": "setting up an empty node js application",
  "main": "src/index.js",
  "repository": "https://github.com/koechkevin/setting-up-node-js.git",
  "author": "koechkevin <koechkevin92@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build-server": "babel src --out-dir dist -s",
    "clean": "rm -rf dist && mkdir dist",
    "build": "yarn run clean && yarn run build-server",
    "serve": "node dist/index.js",
    "start": "NODE_ENV=production yarn run build && yarn run serve",
    "dev": "NODE_ENV=development  DEBUG=log  nodemon --exec babel-node src/index.js",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "seed": "node_modules/.bin/sequelize db:seed:all"
  }
}
